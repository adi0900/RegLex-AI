{
  "version": 2,
  "builds": [
    {
      "src": "src/pipeline/run_pipeline.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/pipeline/run_pipeline.py"
    }
  ],
  "functions": {
    "src/pipeline/run_pipeline.py": {
      "maxDuration": 30
    }
  },
  "env": {
    "ENVIRONMENT": "production"
  }
}